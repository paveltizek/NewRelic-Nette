{
	"name": "vrtak-cz/newrelic-nette",
	"description": "NewRelic PHP agent integration for Nette Framework",
	"type": "library",
	"keywords": ["nette", "newrelic"],
	"license": "MIT",
	"authors": [
		{
			"name": "Patrik Votoček",
			"email": "patrik@votocek.cz",
			"homepage": "http://patrik.votocek.cz",
			"role": "Developer"
		}
	],
	"support": {
		"issues":"https://github.com/Vrtak-CZ/NewRelic-Nette/issues",
		"source":"https://github.com/Vrtak-CZ/NewRelic-Nette"
	},
	"autoload": {
		"psr-4": {
			"VrtakCZ\\NewRelic\\Nette\\": "src/"
		}
	},
	"require": {
		"php": ">=5.6",
		"nette/di": "~2.4 || ^3.0.0",
		"nette/application": "~2.4 || ^3.0.0",
		"vrtak-cz/newrelic-tracy": "^7.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"nella/coding-standard": "4.2.1"
	},
	"archive": {
		"exclude": [
			".gitignore",
			"/.travis.yml",
			"/.travis.composer.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
